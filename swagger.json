                    "example": "$2b$10$c.54dFCQ0fgP7XZvYlh56OF90EIZlOpYPRmDsXk2zcM89o1NjgZWW"
              },
              "profile": {
                "properties": {
                  "id": {
                    "type": "integer",
                    "example": 2
                  },
                  "user_id": {
                      "type": "integer",
                      "example": 2
                    },
                  "identity_type": {
                    "type": "string",
                    "example": "KTP"
                  },
                  "identity_number": {
                    "type": "string",
                    "example": "0777999"
                  },
                  "address": {
                    "type": "string",
                    "example": "Jl. Manchester No. 14"
                  }
                }
              }
            }
          }
        }
      },
      "GetUsersResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 200
          },
          "message": {
            "type": "string",
            "example": "berhasil menampilkan users"
          },
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "example": 2
                },
                "name": {
                  "type": "string",
                  "example": "Lewis user"
                },
                "email": {
                  "type": "string",
                  "example": "lewisuser@gmail.com"
                },
                "password": {
                  "type": "string",
                  "example": "$2b$10$c.54dFCQ0fgP7XZvYlh56OF90EIZlOpYPRmDsXk2zcM89o1NjgZWW"
                }
              }
            }
          }
        }
      },
      "GetUserResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 200
          },
          "message": {
            "type": "string",
