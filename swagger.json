{
  "openapi": "3.0.0",
  "info": {
    "title": "User API",
    "version": "1.0.0",
    "description": "API ini menyediakan serangkaian endpoint untuk mengelola users, accounts dan transactions dalam sistem. Fungsionalitas yang ditawarkan meliputi post,get,put,dan delete. API ini dirancang untuk mendukung aplikasi berbasis web dan seluler, memastikan pengalaman pengguna yang aman dan efisien.  \"berjalan di localhost:3000\""
  },
  "servers": [
    {
      "url": "http://localhost:3000/",
      "description": "Server Localhost"
    }
  ],
  "tags": [
    {
      "name": "Auth",
      "description": "Endpoint untuk authenticate pengguna"
    },
    {
      "name": "Users",
      "description": "Endpoint untuk manajemen pengguna"
    },
    {
      "name": "Accounts",
      "description": "Endpoint untuk manajemen akun"
    },
    {
      "name": "Transactions",
      "description": "Endpoint untuk manajemen transaksi"
    }
  ],
  "paths": {
    "/auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Mendaftarkan pengguna baru",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewAuthUser"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Pengguna berhasil didaftarkan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegisterAuthResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email atau identitas sudah dipakai",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorRegisterResponse400"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Login pengguna",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginAuthRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login sukses",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginAuthResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email atau password salah",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorLoginResponse400"
                }
              }
            }
          }
        }
      }
    },
    "/auth/authenticate": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Autentikasi Pengguna",
        "description": "Endpoint ini digunakan untuk mengautentikasi pengguna menggunakan JWT.",
        "security": [
          {
            "AuthHeader": []
          }
        ],
        "responses": {
          "200": {
            "description": "Pengguna berhasil terautentikasi.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationResponse"
                }
              }
            }
          },
          "401": {
            "description": "Tidak terautentikasi.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/users": {
      "post": {
        "tags": [
          "Users"
        ],
        "summary": "Tambah User",
        "operationId": "addUser",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewUser"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User berhasil ditambahkan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AddUserResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email atau identitas sudah dipakai user lain",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser400"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "Dapatkan Daftar User",
        "operationId": "getUsers",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan users",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetUsersResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/users/{userId}": {
      "parameters": [
        {
          "name": "userId",
          "in": "path",
          "required": true,
          "description": "ID",
          "schema": {
            "type": "integer"
          }
        }
      ],
      "get": {
        "tags": [
          "Users"
        ],
        "summary": "Dapatkan Detail User",
        "operationId": "getUserById",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan detail user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetUserResponse"
                }
              }
            }
          },
          "404": {
            "description": "Tidak ada user dengan ID tersebut",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser404"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Users"
        ],
        "summary": "Update User",
        "operationId": "updateUser",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewUpdateUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User berhasil diupdate",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UpdateUserResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email atau identitas sudah dipakai user lain",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser400"
                }
              }
            }
          },
          "404": {
            "description": "Tidak ada user dengan ID tersebut",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser404"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Users"
        ],
        "summary": "Hapus User",
        "operationId": "deleteUser",
        "responses": {
          "200": {
            "description": "User berhasil dihapus",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeleteUserResponse"
                }
              }
            }
          },
          "404": {
            "description": "Tidak ada user dengan ID tersebut",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser404"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/accounts": {
      "post": {
        "tags": [
          "Accounts"
        ],
        "summary": "Tambah Account",
        "operationId": "addAccount",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewAccount"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Akun berhasil ditambahkan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AddAccountResponse"
                }
              }
            }
          },
          "400": {
            "description": "Nomor bank ini sudah dipakai untuk bank yang sama",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorAccount400"
                }
              }
            }
          },
          "404": {
            "description": "User ID tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorUser404"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Accounts"
        ],
        "summary": "Dapatkan Daftar Account",
        "operationId": "getAccounts",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan daftar akun",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetAccountsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/accounts/{accountId}": {
      "parameters": [
        {
          "name": "accountId",
          "in": "path",
          "required": true,
          "description": "ID",
          "schema": {
            "type": "integer"
          }
        }
      ],
      "get": {
        "tags": [
          "Accounts"
        ],
        "summary": "Dapatkan Detail Account",
        "operationId": "getAccountById",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan detail akun",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetAccountResponse"
                }
              }
            }
          },
          "404": {
            "description": "Akun tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorAccount404"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "Accounts"
        ],
        "summary": "Update Account",
        "operationId": "updateAccount",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewAccountPut"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Akun berhasil diupdate",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UpdateAccountResponse"
                }
              }
            }
          },
          "404": {
            "description": "Akun tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorAccount404"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Accounts"
        ],
        "summary": "Hapus Account",
        "operationId": "deleteAccount",
        "responses": {
          "200": {
            "description": "Akun berhasil dihapus",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeleteAccountResponse"
                }
              }
            }
          },
          "404": {
            "description": "Akun tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorAccount404"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/transactions": {
      "post": {
        "tags": [
          "Transactions"
        ],
        "summary": "Tambah Transaksi",
        "operationId": "addTransaction",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewTransaction"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Transaksi berhasil ditambahkan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AddTransactionResponse"
                }
              }
            }
          },
          "400": {
            "description": "Gagal menambahkan transaksi",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorTransaction400"
                }
              }
            }
          },
          "404": {
            "description": "Akun source atau destination tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorTransactionPost404"
                }
              }
            }
          },
          "422": {
            "description": "Id source dan id destination akun harus berbeda",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorTransaction422"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Transactions"
        ],
        "summary": "Dapatkan Daftar Transaksi",
        "operationId": "getTransactions",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan daftar transaksi",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetTransactionsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/transactions/{transactionId}": {
      "parameters": [
        {
          "name": "transactionId",
          "in": "path",
          "required": true,
          "description": "ID",
          "schema": {
            "type": "integer"
          }
        }
      ],
      "get": {
        "tags": [
          "Transactions"
        ],
        "summary": "Dapatkan Detail Transaksi",
        "operationId": "getTransactionById",
        "responses": {
          "200": {
            "description": "Berhasil menampilkan detail transaksi",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetTransactionIdResponse"
                }
              }
            }
          },
          "404": {
            "description": "Transaksi tidak ditemukan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorTransaction404"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "AuthHeader": {
        "type": "http",
        "scheme": "bearer",
        "description": "Gunakan header `Authorization` dengan format `Bearer <token>` untuk autentikasi anda bisa memasukkan token di sini yang sudah di buat pada melalui login.\n"
      }
    },
    "schemas": {
      "NewAuthUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Lewis register"
          },
          "email": {
            "type": "string",
            "example": "lewisregister@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "lewis7champions"
          },
          "identity_type": {
            "type": "string",
            "example": "KTP"
          },
          "identity_number": {
            "type": "string",
            "example": "0999777"
          },
          "address": {
            "type": "string",
            "example": "Jl. Manchester No. 14"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "identity_type",
          "identity_number",
          "address"
        ]
      },
      "RegisterAuthResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 201
          },
          "message": {
            "type": "string",
            "example": "user berhasil ditambahkan"
          },
          "user": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "example": 1
              },
              "name": {
                "type": "string",
                "example": "Lewis register"
              },
              "email": {
                "type": "string",
                "example": "lewisregister@gmail.com"
              },
              "password": {
                "type": "string",
                "example": "$2b$10$c.54dFCQ0fgP7XZvYlh56OF90EIZlOpYPRmDsXk2zcM89o1NjgZWW"
              },
              "profile": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer",
                    "example": 1
                  },
                  "user_id": {
                    "type": "integer",
                    "example": 1
                  },
                  "identity_type": {
                    "type": "string",
                    "example": "KTP"
                  },
                  "identity_number": {
                    "type": "string",
                    "example": "0999777"
                  },
                  "address": {
                    "type": "string",
                    "example": "Jl. Manchester No. 14"
                  }
                }
              }
            }
          }
        }
      },
      "LoginAuthRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "lewisregister@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "lewis7champions"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginAuthResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "login sukses"
          },
          "token": {
            "type": "string",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MTExMSwibmFtZSI6Ikxld2lzIHJlZ2lzdGVyIiwiaWF0IjoxNzMwMzY2MTU4fQ.TJcXhio187-TEKVBUkeVKtjKf_NMla_gFE0eaX9_ibY"
          }
        }
      },
      "ErrorRegisterResponse400": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 400
          },
          "message": {
            "type": "string",
            "example": "email atau identitas sudah dipakai user lain"
          }
        }
      },
      "ErrorLoginResponse400": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 400
          },
          "message": {
            "type": "string",
            "example": "email atau password salah"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "example": "1070"
          },
          "name": {
            "type": "string",
            "example": "Lewis Hamilton"
          }
        },
        "example": {
          "id": "1070",
          "name": "Lewis Hamilton"
        }
      },
      "AuthenticationResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "selamat anda authenticate!"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "example": {
          "message": "selamat anda authenticate!",
          "user": {
            "id": 1,
            "name": "Lewis register",
            "iat": 1730366158
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "unauthorized"
          }
        },
        "example": {
          "message": "unauthorized"
        }
      },
      "NewUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Lewis user"
          },
          "email": {
            "type": "string",
            "example": "lewisuser@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "lewis7champions"
          },
          "identity_type": {
            "type": "string",
            "example": "KTP"
          },
          "identity_number": {
            "type": "string",
            "example": "0777999"
          },
          "address": {
            "type": "string",
            "example": "Jl. Manchester No. 14"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "identity_type",
          "identity_number",
          "address"
        ]
      },
      "NewUpdateUser": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Lewis update user"
          },
          "email": {
            "type": "string",
            "example": "lewisupdateuser@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "lewis7champions"
          },
          "identity_type": {
            "type": "string",
            "example": "KTP"
          },
          "identity_number": {
            "type": "string",
            "example": "0777999"
          },
          "address": {
            "type": "string",
            "example": "Jl. Manchester No. 14"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "identity_type",
          "identity_number",
          "address"
        ]
      },
      "AddUserResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "example": 201
          },
          "message": {
            "type": "string",
            "example": "user berhasil ditambahkan"
          },
          "user": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "example": 2
              },
              "name": {
                "type": "string",
                "example": "Lewis user"
              },
              "email": {
                "type": "string",
